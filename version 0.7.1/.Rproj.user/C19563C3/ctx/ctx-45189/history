aStarCode <- system.file("extdata", "CN2022.csv", package = "correspondenceTables")
ab <- system.file("extdata", "CN2021_CPA2.1.csv", package = "correspondenceTables")
aaStar <- system.file("extdata", "CN2021_CN2022.csv", package = "correspondenceTables")
# The following code lines call the function "updateCorrespondenceTable"
# of the package "correspondenceTables".
# Use of default values.
updateCorrespondenceTable(aCode, bCode, aStarCode, ab, aaStar, "updateCorrespondenceTable.csv")
# Change the default values.
updateCorrespondenceTable(aCode, bCode, aStarCode, ab, aaStar, "updateCorrespondenceTable.csv", "none", 0.8, 0.9)
# For the Εx Novo Correspondence Table.
# Run the following code lines in order to get the path of the csv files that are included
# in the package "correspondenceTables".
NamesCsv <- system.file("extdata", "names.csv", package = "correspondenceTables")
A <- read.csv(NamesCsv, header = FALSE, sep = ";")
A[1, 1] <- system.file("extdata", "exCN2022.csv", package = "correspondenceTables")
A[1, 2] <- system.file("extdata", "exCN2022_CPA2.1_1.csv", package = "correspondenceTables")
A[2, 2] <- system.file("extdata", "exCPA2.1.csv", package = "correspondenceTables")
A[2, 3] <- system.file("extdata", "exNaceRev2_CPA2.1_1.csv", package = "correspondenceTables")
A[3, 3] <- system.file("extdata", "exNaceRev2.csv", package = "correspondenceTables")
write.table(x = A, file = NamesCsv, row.names = FALSE, col.names = FALSE, sep = ";")
# The following code lines call the function "newCorrespondenceTable"
# of the package "correspondenceTables".
# Use of default values.
newCorrespondenceTable(NamesCsv, "newCorrespondenceTable.csv", 1)
# Change the default values.
newCorrespondenceTable(NamesCsv, "newCorrespondenceTable.csv", 1, "A", 0.3)
