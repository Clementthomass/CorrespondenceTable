A[1, 1] <- system.file("extdata", "exCN2022.csv", package = "correspondenceTables")
A[1, 2] <- system.file("extdata", "exCN2022_CPA2.1_1.csv", package = "correspondenceTables")
A[2, 2] <- system.file("extdata", "exCPA2.1.csv", package = "correspondenceTables")
A[2, 3] <- system.file("extdata", "exNaceRev2_CPA2.1_1.csv", package = "correspondenceTables")
A[3, 3] <- system.file("extdata", "exNaceRev2.csv", package = "correspondenceTables")
write.table(x = A, file = NamesCsv, row.names = FALSE, col.names = FALSE, sep = ";")
